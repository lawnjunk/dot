# Path to nacnud shell lib
# Path to your oh-my-zsh installation.
export ZSH=$HOME/.oh-my-zsh
# setup zsh-complettions
fpath=(/usr/local/share/zsh-completions $fpath)

# setup too version env vars
export V_NODE_VERSION="v4.4.4"
export V_BASH_VERSION="v4.3"
export V_GO_VERSION="V1.6"

# load oh-my-zsh plugins
plugins=(git)
plugins=(colorize)
plugins=(zsh-syntax-highlighting)
source $ZSH/oh-my-zsh.sh

# ~   ~  -  -  ~  -      -   -   - -- >>>>>>---- DOT ---->
source ~/.dot.conf
export DOT_SHELL="zsh"
source $DOT/bin/dot-source.sh

# added by travis gem
[ -f /Users/nacnud/.travis/travis.sh ] && source /Users/nacnud/.travis/travis.sh
